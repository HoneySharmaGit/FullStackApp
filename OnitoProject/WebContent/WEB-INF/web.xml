<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" version="3.0">
  <display-name>OnitoProject</display-name>
  <welcome-file-list>
    <welcome-file>homepage.html</welcome-file>
  </welcome-file-list>
  <servlet-mapping>
  	<servlet-name>TopTenMovies</servlet-name>
  	<url-pattern>/ttm</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>TopTenMovies</servlet-name>
  	<servlet-class>org.onito.servletApp.TopTenMovies</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>AddMovie</servlet-name>
  	<url-pattern>/am</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>AddMovie</servlet-name>
  	<servlet-class>org.onito.servletApp.AddMovie</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>MoviesRating</servlet-name>
  	<url-pattern>/mr</url-pattern>
  </servlet-mapping>
  <servlet>
  	<servlet-name>MoviesRating</servlet-name>
  	<servlet-class>org.onito.servletApp.MoviesRating</servlet-class>
  </servlet>
</web-app>